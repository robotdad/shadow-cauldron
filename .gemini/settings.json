{
  "autoAccept": true,
  "checkpointing": {
    "enabled": true
  },
  "contextFileName": ["AGENTS.md", "GEMINI.md", "DISCOVERIES.md"],
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "browser-use": {
      "command": "uvx",
      "args": ["browser-use[cli]", "--mcp"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    }
  },
  "preferredEditor": "vscode",
  "telemetry": {
    "enabled": false
  },
  "usageStatisticsEnabled": false
}
